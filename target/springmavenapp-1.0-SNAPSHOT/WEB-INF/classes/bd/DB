DROP TABLE IF EXISTS employee;


CREATE TABLE employee (
  id SERIAL NOT NULL,
  first_name varchar(45) DEFAULT NULL,
  last_name varchar(45) DEFAULT NULL,
  email varchar(45) DEFAULT NULL,
  phone varchar(45) DEFAULT NULL,
  PRIMARY KEY (id)
);


